@model IEnumerable<Library.Domain.Models.User>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>title</h2>

<form asp-action="Check" asp-controller="Users">
    @for(var index = 0; index < Model.Count(); index++)
    {
        <div class="form-group">
            <label asp-for="[@index].Email"></label>
            <input asp-for="[@index].Email"/>
        </div>
    }
    
    <input type="submit" value="Send"/>
</form>